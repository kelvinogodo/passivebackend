{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "ACCESS-CONTROL-ALLOW-ORIGIN",
            "value": "*"
          }
        ]
      }
    ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }
  ]
}